// adultRunner >= 18 years old
  // earlyAdultsNumber >= 1000
  // earlyAdultsRunTime 9:30am
  // lateAdultsRunTime 11:00am

// youthRunTime 12:30pm
  // youthRunner < 18 years old
  // allOthersNumber < 1000

const runnersAge = 8; 
const earlyAdult = false;

let earlyAdultsNumber = Math.floor(Math.random() * 1000) + 1000;
let allOthersNumber = Math.floor(Math.random() * 1000);

if(runnersAge < 18) {
  console.log(`Your race number is ${allOthersNumber} and youths run at 12:30pm`);
  } else if(runnersAge >= 18 && !earlyAdult) {
  console.log(`Your race number is ${allOthersNumber} and late registrants run at 11:00am`);
  } else {
  console.log(`Your race number is ${earlyAdultsNumber} and early registrants run at 9:30am`);
}


